{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "icon_128.png"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "trocker.js" ],
      "matches": [ "*://mail.google.com/*", "*://inbox.google.com/*" , "*://*.mail.live.com/*", "*://*.outlook.live.com/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_extension_description__",
   "homepage_url": "https://trockerapp.github.io",
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },
   "manifest_version": 2,
   "name": "__MSG_extension_name__",
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": true,
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "alarms", "tabs", "webNavigation", "webRequest", "webRequestBlocking", "\u003Call_urls>", "*://*.google.com/*", "*://*.live.com/*" ],
   "short_name": "__MSG_extension_name__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3.6",
   "web_accessible_resources": [ "tl.png", "td.png", "tlc.png", "tracked.png", "bypasser.html" ]
}
